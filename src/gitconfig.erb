[user]
  name = Ryan Hansen
  email = ketiko@gmail.com
  editor = gvim
[core]
  autocrlf = true
  whitespace = cr-at-eol
  editor = gvim -f
  excludesfile = ~/.gitignore
[color]
  pager = true
  status = auto
  branch = auto
  diff = auto
  interactive = auto
[push]
  default = upstream
[pull]
  rebase = true
[branch]
  autosetuprebase = local
[clean]
  requireForce = false
[fetch]
  fsckObjects = true
[receive]
  autogc = true
  fsckObjects = true
[help]
  autocorrect = 1
[log]
  abbrevCommit = true
  date = relative
[alias]
  cl = !git clone --template=/c/Tools/Git/eh-git/
  int = !git init --template=/c/Tools/Git/eh-git/ && git config user.name 'Ryan Hansen' && git config user.email 'Ryan.Hansen@ExtendHealth.com'
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset %an' --abbrev-commit --date=relative --decorate
  st = status
  sh = stash
  shs = stash save -u
  shp = stash pop
  ci = commit
  br = branch
  ba = branch -a
  co = checkout
  cb = checkout -b
  df = diff
  dt = difftool
  da = diffall
  di = diff --staged
  ds = diff --stat
  dc = diff --cached
  dtc = difftool --cached
  ca = commit --amend
  aa = add --all
  mg = merge
  mt = mergetool
  rb = rebase
  rbc = rebase --continue
  rba = rebase --abort
  ft = fetch --prune
  fa = fetch --prune --all
  ff = merge --ff-only
  ps = push
  pl = pull --rebase
  pull = pull --rebase
  new = log master.. --reverse
[diff]
  tool = beyondcompare
  guitool = beyondcompare
  renames = copies
[difftool]
  prompt = false
  keepBackup = false
[merge]
  tool = beyondcompare
[mergetool]
  prompt = false
  keepBackup = false
[difftool "beyondcompare"]
  cmd = \"c:/program files (x86)/beyond compare 3/bcomp.exe\" \"$LOCAL\" \"$REMOTE\" -nobackups -title1=\"Old Version\" -title2=\"New Version\"
  trustExitCode = true
[mergetool "beyondcompare"]
  cmd =  \"c:/program files (x86)/beyond compare 3/bcomp.exe\" \"$LOCAL\" \"$REMOTE\" \"$BASE\" \"$MERGED\" -nobackups -title1=\"Local Changes\" -title2=\"Remote Changes\" -title3=\"Committed\"
  trustExitCode = true
