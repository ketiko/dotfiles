source ~/.vimrc
set easymotion
set surround
set multiple-cursors
set argtextobj
set commentary
set ideajoin
nmap <C-P> :action GotoFile<CR>
nmap <Space>f :action SelectInProjectView<CR>
nmap <Space>n :action ActivateProjectToolWindow<CR>
nmap <Left> <Left>
nmap <Down> <Down>
nmap <Up> <Up>
nmap <Right> <Right>
unmap gd
unmap gy
unmap gi
unmap gr
